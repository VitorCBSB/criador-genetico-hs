module genetico(conf_les, conf_outs, chromIn, chromOut);

	input [#r:0][#c:0][#tam_le:0] conf_les;
	input [#num_out:0][#bits_pinos:0] conf_outs;
	input [#num_in:0] chromIn;
	output [#num_out:0] chromOut;
	
	wire [#num_les_1:0] le_out;
	wire [#num_pinos:0] all_inputs;
	
	assign all_inputs = {le_out, chromIn};
	
	assign chromOut = {#all_inputs_for_out};
	
#les

endmodule
